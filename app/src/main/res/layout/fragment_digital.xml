<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    android:fillViewport="true"
   >

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_marginTop="@dimen/_1sdp"
        android:layout_marginRight="@dimen/_5sdp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:weightSum="1">

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_200sdp"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_marginEnd="@dimen/_1sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:padding="@dimen/_5sdp">

                    <ImageView
                        android:id="@+id/preiew_pre_img1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:onClick="analogClockShow"
                        android:src="@drawable/digital_pre_1" />
                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_120sdp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:weightSum="2">

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_marginEnd="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:padding="@dimen/_5sdp">

                    <ImageView
                        android:id="@+id/preiew_pre_img9"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:onClick="analogClockShow"
                        android:src="@drawable/digital_pre_9" />


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:padding="@dimen/_5sdp">

                    <ImageView
                        android:id="@+id/pre_img16"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/contrast"
                        android:onClick="analogClockShow"
                        android:src="@drawable/digital_pre_16" />

                </RelativeLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_120sdp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:weightSum="2">

                <ImageView
                    android:id="@+id/pre_img13_2"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginLeft="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:scaleType="centerCrop"
                    android:background="@color/contrast"
                    android:onClick="analogClockShow"
                    android:src="@drawable/test" />

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_marginEnd="@dimen/_1sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:padding="@dimen/_5sdp">

                    <ImageView
                        android:id="@+id/pre_img15"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:background="@color/contrast"
                        android:onClick="analogClockShow"
                        android:src="@drawable/digital_preview15" />


                </RelativeLayout>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_120sdp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:weightSum="2">


                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_marginEnd="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:padding="@dimen/_5sdp">

                    <ImageView
                        android:id="@+id/preiew_pre_img10"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"

                        android:onClick="analogClockShow"
                        android:padding="@dimen/_10sdp"
                        android:src="@drawable/digital_pre_10" />


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:padding="@dimen/_5sdp">

                    <ImageView
                        android:id="@+id/preiew_pre_img6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:onClick="analogClockShow"
                        android:src="@drawable/digital_pre_6" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_120sdp"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:weightSum="2">

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_marginEnd="@dimen/_1sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:padding="@dimen/_5sdp">

                    <ImageView
                        android:id="@+id/preiew_pre_img7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:onClick="analogClockShow"
                        android:src="@drawable/digital_pre_7" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:padding="@dimen/_5sdp">

                    <ImageView
                        android:id="@+id/preiew_pre_img2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:onClick="analogClockShow"
                        android:src="@drawable/digital_pre_2" />
                </RelativeLayout>


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_120sdp"
                android:orientation="horizontal"
                android:weightSum="2">
                <RelativeLayout
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginStart="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_marginEnd="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:padding="@dimen/_5sdp">

                    <ImageView
                        android:id="@+id/preiew_pre_img8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:onClick="analogClockShow"
                        android:src="@drawable/digital_preview8" />
                </RelativeLayout>
                <ImageView
                    android:id="@+id/preiew_pre_img5"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_marginEnd="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:onClick="analogClockShow"
                    android:src="@drawable/digital_preview5" />


            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_120sdp"
                android:orientation="horizontal"
                android:weightSum="2">


                <ImageView
                    android:id="@+id/preiew_pre_img12"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:onClick="analogClockShow"
                    android:padding="@dimen/_10sdp"
                    android:src="@drawable/digital_preview12" />


                <ImageView
                    android:id="@+id/pre_img13"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginLeft="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:onClick="analogClockShow"
                    android:src="@drawable/test" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/_120sdp"
                android:orientation="horizontal"
                android:weightSum="2">

                <ImageView
                    android:id="@+id/pre_img14"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_marginRight="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:onClick="analogClockShow"
                    android:padding="@dimen/_10sdp"
                    android:src="@drawable/digital_pre_14" />

                <ImageView
                    android:id="@+id/preiew_pre_img4"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:background="@color/contrast"
                    android:onClick="analogClockShow"
                    android:src="@drawable/digital_preview4" />


                <ImageView
                    android:id="@+id/preiew_pre_img3"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_120sdp"
                    android:layout_marginLeft="@dimen/_1sdp"
                    android:layout_marginTop="@dimen/_1sdp"
                    android:layout_weight="1"
                    android:visibility="gone"
                    android:background="@color/contrast"
                    android:onClick="analogClockShow"
                    android:padding="@dimen/_10sdp"
                    android:src="@drawable/digital_preview3" />

            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</RelativeLayout>
